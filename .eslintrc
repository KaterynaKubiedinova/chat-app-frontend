{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": { 
    "no-console": 2,
    "no-useless-escape": 0,
    "no-mixed-spaces-and-tabs": 0,
    "prettier/prettier": 2
  }
}